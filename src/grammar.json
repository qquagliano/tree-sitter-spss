{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "spss",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "command"
          },
          {
            "type": "SYMBOL",
            "name": "injection"
          }
        ]
      }
    },
    "command": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "arguments"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "subcommand"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "end_of_command"
        }
      ]
    },
    "end_of_command": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\.\\n"
      }
    },
    "subcommand": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "name",
            "content": {
              "type": "SYMBOL",
              "name": "subidentifier"
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "arguments"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "arguments": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "keyword"
            },
            {
              "type": "SYMBOL",
              "name": "parenthetical"
            },
            {
              "type": "SYMBOL",
              "name": "variable"
            },
            {
              "type": "SYMBOL",
              "name": "string"
            },
            {
              "type": "SYMBOL",
              "name": "number"
            },
            {
              "type": "SYMBOL",
              "name": "arithmetic"
            },
            {
              "type": "SYMBOL",
              "name": "comparison"
            },
            {
              "type": "SYMBOL",
              "name": "logical"
            },
            {
              "type": "SYMBOL",
              "name": "comma"
            }
          ]
        }
      }
    },
    "parenthetical": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "("
          }
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "keyword"
              },
              {
                "type": "SYMBOL",
                "name": "parenthetical"
              },
              {
                "type": "SYMBOL",
                "name": "variable"
              },
              {
                "type": "SYMBOL",
                "name": "string"
              },
              {
                "type": "SYMBOL",
                "name": "number"
              },
              {
                "type": "SYMBOL",
                "name": "arithmetic"
              },
              {
                "type": "SYMBOL",
                "name": "comparison"
              },
              {
                "type": "SYMBOL",
                "name": "logical"
              },
              {
                "type": "SYMBOL",
                "name": "comma"
              }
            ]
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": ")"
          }
        }
      ]
    },
    "variable": {
      "type": "PATTERN",
      "value": "[A-Za-z_\\-$][A-Za-z0-9_\\-$]*"
    },
    "string": {
      "type": "PATTERN",
      "value": "'[^']*'|\"[^\"]*\""
    },
    "number": {
      "type": "PATTERN",
      "value": "-?(\\d+(\\.\\d*)?|\\.\\d+)"
    },
    "arithmetic": {
      "type": "CHOICE",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "+"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "-"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "*"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "/"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "**"
          }
        }
      ]
    },
    "comparison": {
      "type": "CHOICE",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "<"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": ">"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "="
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "<>"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "<="
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": ">="
          }
        }
      ]
    },
    "logical": {
      "type": "CHOICE",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "&"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "|"
          }
        }
      ]
    },
    "comma": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": ","
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "\\* "
              },
              {
                "type": "PATTERN",
                "value": "[^\\n]*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "\\/\\* "
              },
              {
                "type": "PATTERN",
                "value": "[^\\n]*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "COMMENT"
              },
              {
                "type": "PATTERN",
                "value": "[^\\n]*"
              }
            ]
          }
        ]
      }
    },
    "injection": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "BEGIN PROGRAM"
        },
        {
          "type": "SYMBOL",
          "name": "language"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "injection_source"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "END PROGRAM."
        }
      ]
    },
    "language": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "python"
        },
        {
          "type": "SYMBOL",
          "name": "r"
        }
      ]
    },
    "python": {
      "type": "PATTERN",
      "value": "PYTHON3",
      "flags": "i"
    },
    "r": {
      "type": "PATTERN",
      "value": "R",
      "flags": "i"
    },
    "injection_source": {
      "type": "REPEAT1",
      "content": {
        "type": "PATTERN",
        "value": "[^\\n]+"
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s+"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "identifier"
    },
    {
      "type": "SYMBOL",
      "name": "subidentifier"
    },
    {
      "type": "SYMBOL",
      "name": "keyword"
    }
  ],
  "inline": [],
  "supertypes": []
}
