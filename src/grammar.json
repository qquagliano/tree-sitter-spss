{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "spss",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "command"
      }
    },
    "command": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "keyword"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "subcommand"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "cmd_variable"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\.\\n"
        }
      ]
    },
    "subcommand": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "name",
            "content": {
              "type": "SYMBOL",
              "name": "subidentifier"
            }
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "argument"
            }
          }
        ]
      }
    },
    "argument": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword"
        },
        {
          "type": "SYMBOL",
          "name": "parenthetical"
        },
        {
          "type": "SYMBOL",
          "name": "subcmd_variable"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "equals_assignment"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[A-Z_][A-Z0-9_]*"
    },
    "subidentifier": {
      "type": "PATTERN",
      "value": "\\/[A-Z_][A-Z0-9_]*"
    },
    "keyword": {
      "type": "PATTERN",
      "value": "[A-Z_][A-Z0-9_]*"
    },
    "parenthetical": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "("
          }
        },
        {
          "type": "SYMBOL",
          "name": "argument"
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": ")"
          }
        }
      ]
    },
    "cmd_variable": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "PATTERN",
        "value": "[A-Za-z_][A-Za-z0-9_]*"
      }
    },
    "subcmd_variable": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "PATTERN",
        "value": "[A-Za-z_][A-Za-z0-9_]*"
      }
    },
    "string": {
      "type": "PATTERN",
      "value": "'[^']*'|\"[^\"]*\""
    },
    "number": {
      "type": "PATTERN",
      "value": "-?(\\d+(\\.\\d*)?|\\.\\d+)"
    },
    "equals_assignment": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "="
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "*"
              },
              {
                "type": "PATTERN",
                "value": "[^\\n]*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/*"
              },
              {
                "type": "PATTERN",
                "value": "[^\\n]*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "COMMENT"
              },
              {
                "type": "PATTERN",
                "value": "[^\\n]*"
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
