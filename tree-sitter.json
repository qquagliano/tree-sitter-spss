{
  "grammars": [
    {
      "name": "spss",
      "camelcase": "Spss",
      "scope": "source.spss",
      "path": ".",
      "file-types": [
        "sps",
        ".sps"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^spss$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A treesitter parser for SPSS syntax code",
    "authors": [
      {
        "name": "Quinton Quagliano",
        "email": "Quinton.Quagliano@protonmail.com",
        "url": "https://github.com/qquagliano"
      }
    ],
    "links": {
      "repository": "https://github.com/qquagliano/tree-sitter-spss"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  },
  "theme": {
    "keyword": "white",
    "function": "purple",
    "function.method": "blue",
    "string": "green",
    "comment": "grey",
    "number": "orange"
  }
}
